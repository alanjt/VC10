<?xml version="1.0"?>
<!--
    Autopilot Configuration for the VC10.
-->
<PropertyList>
	<!--
    ============================================================
   Pitch Modes - IAS Hold
    ============================================================
-->

	<!-- IAS_D tracks IAS kts, until IAS mode is engaged, when it is locked-->
	
	<filter>
		<name>IAS leadlag</name>
		<debug>false</debug>
		<type>lead-lag</type>
		<gain>0.03</gain>
		<filter-time-a>0.2</filter-time-a>
		<filter-time-b>2.0</filter-time-b>
		<input>
			<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
		</input>
		<output>
			<property>autopilot/internal/IAS_leadlag</property>
		</output>
	</filter>
	
	<filter>
		<name>IAS_demand</name>
		<debug>false</debug>
		<type>integrator</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<not>
					<property>autopilot/logic/IAS-sw</property>
				</not>
			</condition>
			<expression>
				<product>
					<value>100</value>
					<dif>
						<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
						<property>autopilot/internal/IAS_D</property>
					</dif>
				</product>
			</expression>
		</input>
		<output>
			<property>autopilot/internal/IAS_D</property>
		</output>
		<u_min>0</u_min>
		<u_max>350.0</u_max>
	</filter>
		
	<filter>
		<name>IAS Error</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<dif>
					<property>autopilot/internal/IAS_leadlag</property>
					<property>autopilot/internal/IAS_D</property>
				</dif></expression>
		</input>
		<output><property>autopilot/internal/IAS_D2</property></output>
		<u_min>-50</u_min>
		<u_max>50</u_max>
	</filter>
	
	<filter>
		<name>IAS Mode q feedback</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<product>
					<property>autopilot/gain/Gq_IAS</property>
					<property>autopilot/internal/q-deg_sec</property>
				</product>
			</expression>
		</input>
		<output>autopilot/internal/IAS_q_feedback</output>
		<u_min>-20</u_min>
		<u_max>20</u_max>
	</filter>
	
	<filter>
		<name>IAS Mode Elevator demand</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<sum>
					<property>autopilot/internal/IAS_D</property>
					<property>autopilot/internal/IAS_q_feedback</property>
				</sum>
			</expression>
		</input>
		<output>autopilot/internal/IAS_elevator-deg</output>
		<u_min>-20</u_min>
		<u_max>20</u_max>
	</filter>

	
</PropertyList>
