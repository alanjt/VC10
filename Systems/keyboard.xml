<key n="62">
  <name>Shift + ></name>
  <desc>Pop up autopilot dialog</desc>
  <binding>
		  <command>nasal</command>
		  <script>VC10.changeView(11);</script>
  </binding>
</key>

<key n="79">
  <name>O</name>
  <desc>Extend Refuelling Drogues</desc>  
  <binding>
		  <command>nasal</command>
		  <script>VC10.toggleRefuelling();</script>
  </binding>
</key>
<key n="2">
  <name>Ctrl + b</name>
  <desc>Speedbrakes/Spoiler full</desc>  
  <binding>
		  <command>nasal</command>
		  <script>VC10.fullSpeedbrakes();</script>
  </binding>
</key>

</PropertyList>
