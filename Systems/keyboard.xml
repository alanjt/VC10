<PropertyList>
<key n="127">
	<name>Delete</name>
	<desc>Toggle Reversers</desc>
  <binding>
      <command>nasal</command>
      <script>VC10.togglereverser();</script>
  </binding>
</key>
<key n="60">
  <name> > </name>
  <desc>toggle pilot engineer view</desc>
  <binding>
		  <command>nasal</command>
		  <script>VC10.changeView(9);</script>
  </binding>
</key>

<key n="108">
  <name>L</name>
  <desc>toggle landing lights</desc>
  <binding>
      <command>nasal</command>
      <script>VC10.toggleLandingLights();</script>
  </binding>
</key>
<key n="121">
  <name>y</name>
  <desc>toggle yokes</desc>
  <binding>
      <command>property-toggle</command>
      <property>sim/yokes-visible</property>
  </binding>
</key>

<key n="94"> <!-- ^ -->
    <name>short start</name>
    <desc>Test startup</desc>
  <binding>
      <command>nasal</command>
      <script>VC10.short_startup();</script>
  </binding>
</key>

<key n="257">
    <name>F1</name>
    <desc>Checklists</desc>
    <binding>
      <command>dialog-show</command>
      <dialog-name>checklist</dialog-name>
    </binding>
</key>

<key n="263">
	<name>F7</name>
	<desc>Show map</desc>
	<binding>
	<command>dialog-show</command>
	<dialog-name>map</dialog-name>
	</binding>
</key>

<key n="264">
	<name>F8</name>
	<desc>Show map</desc>
	<binding>
	<command>dialog-show</command>
	<dialog-name>route-manager</dialog-name>
	</binding>
</key>

<key n="265">
    <name>F9</name>
    <desc>Pop up Fuel and Weight dialog</desc>
    <binding>
        <command>nasal</command>
        <script>VC10.WeightFuelDialog()</script>
    </binding>
</key>
