 <aerodynamics>
 
	<function name="inertia/cg-x-percent">
		<description>cg, percentage of cbar</description>
		<quotient>
			<difference>
				<property>inertia/cg-x-in</property>
				<value>872.0</value>
				<!-- SMC LE is 872 inches aft of fuselage datum-->
			</difference>
			<value>2.402</value>
			<!-- SMC is 240.2 inches, divide by 100 to get % -->
		</quotient>
	</function>

