<PropertyList>

 <fx>
 
  <engine>
		<name>startupOne</name>
 		<path>Sounds/whine.wav</path>
		<mode>looped</mode>
		<condition>
			<property>engines/engine[0]/n1</property>
			<or>
				<and>
					<not><property>sim/current-view/internal</property></not>
					<not>
						<equals>
							<property>sim/current-view/name</property>
							<value type="string">Passengers View</value>
						</equals>
					</not>					
					<not>
						<equals>
							<property>sim/current-view/view-number</property>
							<value>12</value>	
						</equals>
					</not>
				</and>
				<greater-than>
					<property>instrumentation/doors/pilotwin/position-norm</property>
					<value>0.01</value>
				</greater-than>
			</or>
		</condition>			    
		<volume>
			<property>engines/engine[0]/n1</property>
			<type>ln</type>
			<factor>0.18</factor>
			<offset>0.05</offset>
		</volume>
		<pitch>
			<property>engines/engine[0]/n1</property>
			<type>log</type>
			<factor>1.2</factor>
			<offset>0.10</offset>
		</pitch>
		<position>
			<x>4.5</x>
			<y>4.7</y>
			<z>1.2</z>
		</position>
		<orientation>
			<x>-1 </x>
			<y> 0 </y>
			<z> 0 </z>
			<inner-angle>180</inner-angle>
			<outer-angle>360</outer-angle>
			<outer-gain>0.5</outer-gain>
			<reference-dist>20</reference-dist>
		</orientation>
  </engine>
 
