	<stall>
		<name>stall or overspeed</name>
		<path>Aircraft/VC10/Sounds/speed.wav</path>		
		<condition>
			<and>
				<greater-than>
					<property>VC10/electric/ess-bus</property>
					<value>20</value>
				</greater-than>
				<or>
					<property>controls/test/warn-knob</property>
					<and>
						<less-than>
							<property>velocities/airspeed-kt</property>
							<value>120.0</value>
						</less-than>
						<greater-than>
							<property>position/altitude-agl-ft</property>
							<value>600.0</value>					
						</greater-than>
					</and>
					<greater-than>
						<property>velocities/mach</property>
						<value>0.9</value>
					</greater-than>
				</or>
			</and>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</stall>

