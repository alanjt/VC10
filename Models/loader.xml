<PropertyList>

	<path>loader.ac</path>
	
  <animation>
    <type>material</type>
			<object-name>jourContainerDeck</object-name>
			<object-name>jourContainerDeck.001</object-name>
    	<property-base>sim/model/livery</property-base>
    	<texture-prop>texture-container</texture-prop>
    <texture>container.png</texture>
  </animation>
 
	<animation>
		<type>material</type>
		<object-name>jour</object-name> 
		<object-name>jourPlatform</object-name>
		<object-name>jourContainerDeck</object-name>
		<object-name>jourShear</object-name>
		<object-name>jourShear.001</object-name>
		<object-name>jourPlatform.001</object-name>
		<object-name>jourContainerDeck.001</object-name>
		<object-name>jourShear.002</object-name>
		<object-name>jourShear.003</object-name>
		<object-name>jourCylinder</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.04</red>
		  <green>0.04</green>
		  <blue>0.04</blue>
		  <factor-prop>sim/multiplay/generic/float[0]</factor-prop>
		</emission>
	</animation>
	
  <animation>
      <type>translate</type> 
			<object-name>jourCylinder</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0 </ind><dep>  -1.3</dep></entry>
				<entry><ind>    0.08</ind><dep>  -1.3</dep></entry>
				<entry><ind>    0.5</ind><dep>   0.0</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <axis>
          <x> 0.0</x>
          <y> 0.0</y>
          <z> 1.0</z>
      </axis>
  </animation>
	
  <animation>
      <type>translate</type> 
			<object-name>jourPlatform</object-name>
			<object-name>jourContainerDeck</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep>  -2.0</dep></entry>
				<entry><ind>    0.1</ind><dep>  -2.0</dep></entry>
				<entry><ind>    0.5</ind><dep>   0.0</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <axis>
          <x> 0.0</x>
          <y> 0.0</y>
          <z> 1.0</z>
      </axis>
  </animation>

  <animation>
      <type>translate</type> 
			<object-name>jourShear</object-name>
			<object-name>jourShear.001</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep>  -1.0</dep></entry>
				<entry><ind>    0.1</ind><dep>  -1.0</dep></entry>
				<entry><ind>    0.5</ind><dep>   0.0</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <axis>
          <x> 0.0</x>
          <y> 0.0</y>
          <z> 1.0</z>
      </axis>
  </animation>
	
  <animation>
      <type>rotate</type>
      <object-name>jourShear.001</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep>  45.0</dep></entry>
				<entry><ind>    0.1</ind><dep>  45.0</dep></entry>
				<entry><ind>    0.5</ind><dep>   0.0</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <center>
          <x-m> 0.0</x-m>
          <y-m>-3.8</y-m>
          <z-m>-1.12</z-m>
      </center>
      <axis>
          <x>1</x>
          <y>0</y>
          <z>0</z>
      </axis>
  </animation>
  
  <animation>
      <type>rotate</type>
      <object-name>jourShear</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep> -45.0</dep></entry>
				<entry><ind>    0.1</ind><dep> -45.0</dep></entry>
				<entry><ind>    0.5</ind><dep>   0.0</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <center>
          <x-m> 0.0</x-m>
          <y-m>-3.8</y-m>
          <z-m>-1.12</z-m>
      </center>
      <axis>
          <x>1</x>
          <y>0</y>
          <z>0</z>
      </axis>
  </animation>
	
  <animation>
      <type>translate</type> 
			<object-name>jourPlatform.001</object-name>
			<object-name>jourContainerDeck.001</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep>  -2.6</dep></entry>
				<entry><ind>    0.5</ind><dep>  -2.6</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <axis>
          <x> 0.0</x>
          <y> 0.0</y>
          <z> 1.0</z>
      </axis>
  </animation>  

  <animation>
      <type>translate</type> 
			<object-name>jourShear.002</object-name>
			<object-name>jourShear.003</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep>  -1.4</dep></entry>
				<entry><ind>    0.5</ind><dep>  -1.4</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <axis>
          <x> 0.0</x>
          <y> 0.0</y>
          <z> 1.0</z>
      </axis>
  </animation>
  
  <animation>
      <type>rotate</type>
      <object-name>jourShear.002</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep>  37.0</dep></entry>
				<entry><ind>    0.5</ind><dep>  37.0</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <center>
          <x-m> 0.0</x-m>
          <y-m>-7.77</y-m>
          <z-m>-1.21</z-m>
      </center>
      <axis>
          <x>1</x>
          <y>0</y>
          <z>0</z>
      </axis>
  </animation>
  
  <animation>
      <type>rotate</type>
      <object-name>jourShear.003</object-name>
      <property>sim/multiplay/generic/float[15]</property>
			<interpolation>
				<entry><ind>    0.0</ind><dep> -37.0</dep></entry>
				<entry><ind>    0.5</ind><dep> -37.0</dep></entry>
				<entry><ind>    1.0</ind><dep>   0.0</dep></entry>
			</interpolation>
      <center>
          <x-m> 0.0</x-m>
          <y-m>-7.77</y-m>
          <z-m>-1.21</z-m>
      </center>
      <axis>
          <x>1</x>
          <y>0</y>
          <z>0</z>
      </axis>
  </animation>
		
</PropertyList>
