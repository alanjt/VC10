<PropertyList>
	<path>EngineerStation.ac</path>

	
	<model>
		<name>engineer-panels</name>
		<path>Aircraft/VC10/Models/Cockpit/Panels/EngineerPanels/EngineerPanels.xml</path>
		<offsets>
			<x-m> 0</x-m>
			<y-m> 0</y-m>
            <z-m> 0</z-m>
			<pitch-deg>10</pitch-deg>
		</offsets>
	</model>
	
	
	<model>
		<name>Engineer Rear Bulkhead Panels</name>
		<path>Aircraft/VC10/Models/Cockpit/Panels/EngineerPanels/RearPanel/RearEngineerPanel.xml</path>
		<offsets>
			<x-m>0.080</x-m>
			<y-m>1.065</y-m>
			<z-m>0</z-m>
			<pitch-deg>0</pitch-deg>
			<heading-deg>-90</heading-deg>
		</offsets>
	</model>


</PropertyList>
