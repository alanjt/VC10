<PropertyList>

	<path>Aircraft/VC10/Models/Cockpit/SwitchesLamps/magnetic_indicator_round.ac</path>

	<!-- GCB indicator 2 -->
	<animation>
		<type>textranslate</type>
		<object-name>ball</object-name>
	<!--<property>VC10/electric/ac/generator/GCB2-ind</property> -->
		<property>VC10/electric/ac/generator/GCB2</property> -->
		<interpolation>
			<entry><ind>0</ind><dep>0.2</dep></entry>
			<entry><ind>1.0</ind><dep>0</dep></entry>
		</interpolation>
		<bias>0.001</bias>
		<factor>-0.1</factor>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>


</PropertyList>
