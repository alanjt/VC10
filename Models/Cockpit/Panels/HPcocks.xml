<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
 <path>HPcocks.ac</path>
 
	<animation>
		<type>material</type>
		<object-name>pedestal</object-name>  
		<object-name>HPcock.001</object-name>  
		<object-name>HPcock.002</object-name>  
		<object-name>HPcock.003</object-name>  
		<object-name>HPcock.004</object-name>  
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.25</red>
		  <green>0.25</green>
		  <blue>0.25</blue>
		  <factor-prop>systems/electrical/outputs/cabin-dim</factor-prop>
		</emission>
	</animation> 

	
	<!-- HPcock levers -->
	<animation>
		<type>rotate</type>
		<object-name>HPcock.001</object-name>
		<property>VC10/engines/HPcock[0]</property>
		<factor>55</factor>
		<center>
			<x-m>  0.0571</x-m>
			<y-m>  0.0</y-m>
			<z-m>  -0.1292</z-m>
		</center>
		<axis>
		  <x>0.0</x>
		  <y>1.0</y>
		  <z>0.0</z>
		</axis>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>HPcock.001</object-name>
		<action>
		  <button>0</button>
		  <repeatable>false</repeatable>
		  <binding>
		    <command>property-toggle</command>
		    <property>VC10/engines/HPcock[0]</property>
		  </binding>
		</action>
	</animation>
	
