<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
 <path>HPcocks.ac</path>
 
	<animation>
		<type>material</type>
		<object-name>pedestal</object-name>  
		<object-name>HPcock.001</object-name>  
		<object-name>HPcock.002</object-name>  
		<object-name>HPcock.003</object-name>  
		<object-name>HPcock.004</object-name>  
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.25</red>
		  <green>0.25</green>
		  <blue>0.25</blue>
		  <factor-prop>systems/electrical/outputs/cabin-dim</factor-prop>
		</emission>
	</animation> 

	<!-- HPcock levers -->
	
	<animation>
		<type>rotate</type>
		<object-name>HPcock.001</object-name>
		<property>VC10/engines/HPcock[0]</property>
		<interpolation>
			<entry><ind>0</ind><dep>55</dep></entry>
			<entry><ind>0.75</ind><dep>15</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
		</interpolation>
		<center>
			<x-m>  0.0571</x-m>
			<y-m>  0.0</y-m>
			<z-m>  -0.1292</z-m>
		</center>
		<axis>
		  <x>0.0</x>
		  <y>1.0</y>
		  <z>0.0</z>
		</axis>
	</animation>
	
	<animation>
		<name>HP Cock 1 </name>
		<type>slider</type>
		<object-name>HPcock.001</object-name>
		<property>VC10/engines/HPcock[0]</property>
		<drag-direction>vertical</drag-direction>
		<action>
			<binding>
				<command>property-cycle</command>
				<property>VC10/engines/HPcock[0]</property>
				<value>0</value>
				<value>0.75</value>
				<value>1.0</value>
				<wrap>false</wrap>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<label>HP Cock 1 : %.2f</label>
				<tooltip-id>no</tooltip-id>
				<property>VC10/engines/HPcock[0]</property>
			</binding>
		</hovered>
	</animation>

