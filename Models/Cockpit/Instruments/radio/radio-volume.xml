	  <object-name>LampWhiteON.006</object-name>
	  <object-name>LampWhiteON.007</object-name>
	  <object-name>LampWhiteON.008</object-name>
	  <emission>
	    <red>   0.8 </red>
	    <green> 0.8</green>
	    <blue>  0.5 </blue>
	  </emission>
	</animation>

	<!-- Comm[0] -->
	<animation>
		<type>select</type>
		<object-name>LampWhiteON</object-name>
		<condition>
			<greater-than>
				<property>VC10/electric/ess-bus</property>
				<value>20</value>
			</greater-than>
			<or>
				<and>
					<property>instrumentation/comm[0]/serviceable</property>
					<property>instrumentation/comm[0]/power-btn</property>
					<greater-than>
						<property>instrumentation/comm[0]/signal-quality-norm</property>
						<value>0.4</value>
					</greater-than>
				</and>
				<property>controls/test/warn-knob</property>
			</or>
		</condition>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>knob</object-name>
		<property>instrumentation/comm[0]/volume</property>
		<factor>-270</factor>
		<center>
			<x-m> 0.0</x-m>
			<y-m>-0.1</y-m>
			<z-m> 0.0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>knob</object-name>
		<visible>true</visible>
		<action>
			<name>calibrate fine dec</name>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/comm[0]/volume</property>
				<step>0.1</step>
				<min>0.0</min>
				<max>1.0</max>
				<wrap>false</wrap>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>sim/sound/switch3</property>
			</binding>
		</action>
