	<!-- Vibration Test -->
	<animation>
		  <type>rotate</type>
		  <object-name>switch</object-name>
		  <property>VC10/vibrations/vib-test</property>
      <factor>-60</factor>
		  <axis>
		   <x>0</x>
		   <y>0</y>
		   <z>1</z>
		  </axis>
		  <center>
		  <x-m> 0.0417</x-m>
		  <y-m>-0.0025</y-m>
		  <z-m> 0.0434</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>switch</object-name>
		  <object-name>sockle</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
		  			<property>VC10/vibrations/vib-test</property>
		    </binding>
        <mod-up>
          <binding>
		        <command>property-toggle</command>
		  			<property>VC10/vibrations/vib-test</property>
          </binding>
          <binding>
		        <command>nasal</command>
		  			<script>VC10.eng_vib();</script>
          </binding>
        </mod-up>         
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
