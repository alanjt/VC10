	<!-- inter connect brake valve -->
	<animation>
		  <type>rotate</type>
		  <object-name>switch.003</object-name>
		  <property>VC10/hydraulic/brake-valve</property>
		 	<interpolation>
				<entry><ind>  1.0</ind><dep> -50.0</dep></entry>
				<entry><ind>  0.0</ind><dep>   0.0</dep></entry>
				<entry><ind>  2.0</ind><dep>  50.0</dep></entry>
			</interpolation>  
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0</x-m>
		  <y-m>0.005</y-m>
		  <z-m>0.0023</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>plane.001</object-name>
		  <visible>false</visible>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>
            	var bt = getprop("VC10/hydraulic/brake-valve") or 0;
            	if(bt > 0){
            		setprop("VC10/hydraulic/brake-valve", 0);
            	}else{
            		setprop("VC10/hydraulic/brake-valve", 1);
            		<!-- fall back, if ac aux pumps or ess-bus to low -->
            		var acaux1 = getprop("VC10/hydraulic/ac-aux-pump[0]") or 0;
            		var acaux2 = getprop("VC10/hydraulic/ac-aux-pump[1]") or 0;
            		
            		if((getprop("VC10/electric/ess-bus") != nil and getprop("VC10/electric/ess-bus") &lt; 20.0) or !acaux1 or !acaux2){
            			settimer(func{
            				setprop("VC10/hydraulic/brake-valve", 0);
									}, 0.5);
								 	if(getprop("/sim/sound/switch2") == 1){
										 setprop("/sim/sound/switch2", 0); 
									}else{
										 setprop("/sim/sound/switch2", 1);
									}
            		}
            	}
            </script>
        </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
	<animation>
		  <type>pick</type>
		  <object-name>plane.002</object-name>
		  <visible>false</visible>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>
            	var bt = getprop("VC10/hydraulic/brake-valve") or 0;
            	if(bt > 0){
            		setprop("VC10/hydraulic/brake-valve", 0);
            	}else{
            		setprop("VC10/hydraulic/brake-valve", 2);
            		<!-- fall back, if ac aux pumps or ess-bus to low -->
            		var acaux1 = getprop("VC10/hydraulic/ac-aux-pump[0]") or 0;
            		var acaux2 = getprop("VC10/hydraulic/ac-aux-pump[1]") or 0;
            		
            		if((getprop("VC10/electric/ess-bus") != nil and getprop("VC10/electric/ess-bus") &lt; 20.0) or !acaux1 or !acaux2){
            			settimer(func{
            				setprop("VC10/hydraulic/brake-valve", 0);
									}, 0.5);
								 	if(getprop("/sim/sound/switch2") == 1){
										 setprop("/sim/sound/switch2", 0); 
									}else{
										 setprop("/sim/sound/switch2", 1);
									}
            		}
            	}
            </script>
        </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
