  <!-- engine vibration selector --> 
	<animation>
		<type>rotate</type>
		<object-name>turn</object-name>
		<property>VC10/vibrations/vib-sel</property>
		<interpolation>
		  <entry><ind> 0</ind><dep>   0</dep></entry>
		  <entry><ind> 1</ind><dep>  45</dep></entry>
		  <entry><ind> 2</ind><dep>  90</dep></entry>
		</interpolation>
		<axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.0402</x-m>
		<y-m> 0.0</y-m>
		<z-m> 0.0</z-m>
		</center>  
	</animation>
     
	<animation>
		<type>pick</type>
		<object-name>turn</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
				<property>VC10/vibrations/vib-sel</property>
		    <step>1</step>
		    <min>0</min>
		    <max>3</max>
		    <wrap>1</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>1</button>
		  <repeatable>true</repeatable>
		  <binding>
