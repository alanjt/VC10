		    <min>-1900</min>
		    <max> 1900</max>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>4</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
				<property>VC10/pressurization/manual-control-selector</property>
		    <step>-100</step>
		    <min>-1900</min>
		    <max> 1900</max>
		    <wrap>0</wrap>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
