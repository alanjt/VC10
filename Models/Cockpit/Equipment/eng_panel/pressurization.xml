  <animation>
    <type>pick</type>
      <object-name>switch.003</object-name>
      <object-name>sockle.003</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
        <binding>
          <command>property-assign</command>
          <property>VC10/pressurization/cabin-alt-warning</property>  
          <value>1.0</value>
        </binding>        
        <binding>
          <command>property-assign</command>
          <property>VC10/pressurization/alt-cutout-horn</property>  
          <value>1.0</value>
        </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>VC10/pressurization/cabin-alt-warning</property>  
          <value>0.0</value>
        </binding>
      </mod-up>
    </action>
  </animation>

  
  <!-- cabin air temperature source selector --> 
	<animation>
		<type>rotate</type>
		<object-name>turn</object-name>
		<property>VC10/pressurization/cabin-air-temp-selector</property>
		<interpolation>
		  <entry><ind> 0</ind><dep>   0</dep></entry>
		  <entry><ind> 1</ind><dep>  41</dep></entry>
		  <entry><ind> 2</ind><dep>  76</dep></entry>
		  <entry><ind> 3</ind><dep> 110</dep></entry>
		</interpolation>
		<axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.2805</x-m>
		<y-m> 0.0</y-m>
		<z-m>-0.1085</z-m>
		</center>  
	</animation>
     
	<animation>
		<type>pick</type>
		<object-name>turn</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
				<property>VC10/pressurization/cabin-air-temp-selector</property>
		    <step>1</step>
		    <min>0</min>
		    <max>4</max>
		    <wrap>1</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>1</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
				<property>VC10/pressurization/cabin-air-temp-selector</property>
		    <step>-1</step>
		    <min>0</min>
		    <max>4</max>
		    <wrap>1</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
		</action>
	</animation>
	
  <!-- manual mode switch -->
	<animation>
		  <type>rotate</type>
		  <object-name>switch</object-name>
		  <property>VC10/pressurization/manual-mode-switch</property>
      <factor>-60</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m> 0.0</x-m>
		  <y-m>-0.001</y-m>
		  <z-m>-0.163</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>switch</object-name>
		  <object-name>sockle</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
		        <property>VC10/pressurization/manual-mode-switch</property>
		    </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
  <!-- manual control selector --> 
