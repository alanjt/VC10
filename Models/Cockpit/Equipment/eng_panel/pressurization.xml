		<type>rotate</type>
		<object-name>knob.004</object-name>
		<property>VC10/pressurization/manual-control-selector</property>
		<interpolation>
		  <entry><ind>-1900</ind><dep> -155</dep></entry>
		  <entry><ind> 0</ind><dep>   0</dep></entry>
		  <entry><ind> 1900</ind><dep>  155</dep></entry>
		</interpolation>
		<axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.2303</x-m>
		<y-m> 0.0</y-m>
		<z-m>-0.1961</z-m>
		</center>  
	</animation>
     
	<animation>
		<type>pick</type>
		<object-name>knob.004</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-assign</command>
				<property>VC10/pressurization/manual-control-selector</property>
		    <value>0</value>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
