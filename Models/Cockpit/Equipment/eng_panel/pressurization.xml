  <!-- outflow valve balance control --> 
	<animation>
		<type>rotate</type>
		<object-name>knob.003</object-name>
		<property>VC10/pressurization/outflow-valve-balance-control</property>
		<factor>90</factor>
		<axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.1818</x-m>
		<y-m> 0.0</y-m>
		<z-m>-0.1997</z-m>
		</center>  
	</animation>
     
	<animation>
		<type>pick</type>
		<object-name>knob.003</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-assign</command>
				<property>VC10/pressurization/outflow-valve-balance-control</property>
		    <value>0</value>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>3</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
				<property>VC10/pressurization/outflow-valve-balance-control</property>
		    <step>0.05</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>4</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
				<property>VC10/pressurization/outflow-valve-balance-control</property>
		    <step>-0.05</step>
		    <min>-1</min>
		    <max>1</max>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/sound/switch3</property>
		  </binding>
		</action>
	</animation>
	
  <!-- outflow valve forward --> 
