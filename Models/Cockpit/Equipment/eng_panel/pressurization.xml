<PropertyList>
 <path>pressurization.ac</path>

	<animation>
		<type>material</type>
		<object-name>cover</object-name>
		<object-name>LampAmber</object-name>
		<object-name>bezel</object-name> 
		<object-name>knob</object-name> 
		<object-name>knob.001</object-name>
		<object-name>knob.002</object-name>
		<object-name>knob.003</object-name>
		<object-name>knob.004</object-name>
		<object-name>switch</object-name> 
		<object-name>switch.001</object-name>
		<object-name>switch.002</object-name>
		<object-name>switch.003</object-name>
		<object-name>switch.004</object-name>
		<object-name>sockle</object-name> 
		<object-name>sockle.001</object-name>
		<object-name>sockle.002</object-name>
		<object-name>sockle.003</object-name>
		<object-name>sockle.004</object-name>
		<object-name>sockle.005</object-name>
		<object-name>turn</object-name>
		<condition>			
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.2</red>
		  <green>0.2</green>
		  <blue>0.2</blue>
		  <factor-prop>systems/electrical/outputs/engineer-dim</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>line</object-name>
		<object-name>face</object-name>
		<object-name>face.001</object-name>
		<object-name>face_alt</object-name>
		<object-name>face_baro</object-name>
		<object-name>face_psi</object-name>
		<object-name>Needle</object-name>
		<object-name>Needle.001</object-name>
		<condition>			
			<less-than><property>systems/electrical/outputs/cabin-dim</property><value>0.8</value></less-than>
		</condition>
		<emission>
			<red>0.6</red>
			<green>0.4</green>
			<blue>0.3</blue>
			<factor-prop>systems/electrical/outputs/engineer-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>line</object-name>
		<object-name>face</object-name>
		<object-name>face.001</object-name>
		<object-name>face_alt</object-name>
		<object-name>face_baro</object-name>
		<object-name>face_psi</object-name>
		<object-name>Needle</object-name>
		<object-name>Needle.001</object-name>
		<condition>			
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<property>systems/electrical/outputs/engineer-dim</property>
		</condition>
		<emission>
		  <red>0.8</red>
		  <green>0.8</green>
		  <blue>0.8</blue>
		  <factor-prop>systems/electrical/outputs/engineer-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>LampAmberON</object-name>
		<emission>
		  <red>   1.0 </red>
		  <green> 0.8 </green>
		  <blue>  0.4 </blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>LampAmberON</object-name>
		<condition>		
			<greater-than>
				<property>VC10/electric/ess-bus</property>
				<value>20</value>
			</greater-than>
		  <or>
		  	<not><property>VC10/pressurization/safety-valve-pos</property></not>
		    <property>controls/test/warn-knob</property>
		  </or>
		</condition>
	</animation>
	
	<!-- Safety Valve Switch on Auxilliary Panel -->
  <animation>
    <type>rotate</type>
    <object-name>cover</object-name>
    <property>VC10/pressurization/safety-valve-cover</property>
    <interpolation> 
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-90.0</dep>
            </entry>		
        </interpolation>
    <axis>
     <x> 1</x>
     <y> 0</y>
     <z> 0</z>
    </axis>
    <center>
    <x-m> 0.0</x-m>
    <y-m>-0.0002</y-m>
    <z-m> 0.0525</z-m>
    </center>  
  </animation>

	<animation>
		  <type>pick</type>
		  <object-name>cover</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
		        <property>VC10/pressurization/safety-valve-cover</property>
		    </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch3</property>
		    </binding>
		  </action>
	</animation>
	
  <!-- safety valve -->
	<animation>
		  <type>rotate</type>
		  <object-name>switch.004</object-name>
		  <property>VC10/pressurization/safety-valve</property>
      <factor> 85</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m> 0.0</x-m>
		  <y-m>-0.001</y-m>
		  <z-m>0.0376</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>switch.004</object-name>
		  <object-name>sockle.005</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
		        <property>VC10/pressurization/safety-valve</property>
		    </binding>
        <binding>
            <command>nasal</command>
            <script>VC10.safety_valv_pos();</script>
        </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
  <!-- air thrust valve -->
	<animation>
		  <type>rotate</type>
		  <object-name>switch.002</object-name>
		  <property>VC10/pressurization/air-thrust-valve</property>
      <factor>-60</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m> 0.0</x-m>
		  <y-m>-0.001</y-m>
		  <z-m>-0.182</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>switch.002</object-name>
		  <object-name>sockle.002</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
		        <property>VC10/pressurization/air-thrust-valve</property>
		    </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
  <!-- cabin altitude warning -->
  <animation>
      <type>translate</type>
      <object-name>switch.003</object-name>
      <property>VC10/pressurization/cabin-alt-warning</property>
      <factor>0.0028</factor>
      <axis>
          <x> 0.0</x>
          <y> 1.0</y>
          <z> 0.0</z>
      </axis>
      <center>
          <x-m> 0.3014</x-m>
          <y-m> 0.0008</y-m>
          <z-m>-0.154</z-m>
      </center>
  </animation>
  
