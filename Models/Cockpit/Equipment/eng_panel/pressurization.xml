		<axis>
		 <x>1</x>
		 <y>0</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.1369</x-m>
		<y-m> 0.09654</y-m>
		<z-m>-0.1689</z-m>
		</center>  
	</animation>		
	
	<!-- baro correction knob -->
	<animation>
		<type>rotate</type>
		<object-name>knob.002</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>-1000</factor>
		<axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.1359</x-m>
		<y-m> 0.0</y-m>
		<z-m>-0.1997</z-m>
		</center>  
	</animation>
	
  <animation>
      <type>pick</type>
    	<object-name>knob.002</object-name>
      <visible>true</visible>      
      <action>
	      <name>show info</name>
	      <button>0</button>
	    	<repeatable>true</repeatable>			
	      <binding>
					<command>nasal</command>
					<script>VC10.show_alti()</script>
				</binding>
	    </action>
      <action>
          <button>3</button>
          <binding>
          <command>property-adjust</command>
          <property>instrumentation/altimeter/setting-inhg</property>
          <step>0.01</step>
          <min>29.0</min>
          <max>31.0</max>
          <wrap>false</wrap>
          </binding>
      </action>
      <action>
          <button>4</button>
          <binding>
          <command>property-adjust</command>
          <property>instrumentation/altimeter/setting-inhg</property>
          <step>-0.01</step>
          <min>29.0</min>
          <max>31.0</max>
          <wrap>false</wrap>
          </binding>
      </action>
  </animation>	
  
	<!-- rotate aircrafts altitude scale -->
	<animation>
		<type>rotate</type>
		<object-name>face_alt</object-name>
    <property>instrumentation/altimeter/indicated-altitude-ft</property>
		<interpolation>
		  <entry><ind> -200.0</ind><dep> -2.5</dep></entry>
		  <entry><ind>    0.0</ind><dep>  0.0</dep></entry>
		  <entry><ind> 5000.0</ind><dep> 12.7</dep></entry>
		  <entry><ind>10000.0</ind><dep> 25.0</dep></entry>
		  <entry><ind>15000.0</ind><dep> 38.1</dep></entry>
		  <entry><ind>20000.0</ind><dep> 50.8</dep></entry>
		  <entry><ind>25000.0</ind><dep> 63.3</dep></entry>
		  <entry><ind>47000.0</ind><dep>119.1</dep></entry>
		</interpolation>
		<axis>
		 <x>1</x>
		 <y>0</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.0</x-m>
		<y-m> 0.09654</y-m>
		<z-m>-0.1631</z-m>
		</center>  
	</animation>
	
	<!-- rotate cabin altitude scale -->
	<animation>
		<type>rotate</type>
		<object-name>face_psi</object-name>
		<property>VC10/pressurization/cabin-max</property>
		<interpolation>
		  <entry><ind> -200.0</ind><dep> -2.0</dep></entry>
		  <entry><ind>    0.0</ind><dep>  0.0</dep></entry>
		  <entry><ind> 9000.0</ind><dep> 57.1</dep></entry>
		  <entry><ind> 9100.0</ind><dep> 63.0</dep></entry>
		  <entry><ind>55000.0</ind><dep> 63.0</dep></entry>
		</interpolation>
