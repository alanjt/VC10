	<!-- Open cover -->
	<animation>
		  <type>rotate</type>
		  <object-name>cover</object-name>
		  <object-name>scharnier</object-name>
		  <property>VC10/fuel/valves/dump-cover[0]</property>
      <factor>140</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0</x-m>
		  <y-m>-0.013</y-m>
		  <z-m>-0.052</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>cover</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
            <command>nasal</command>
            <script>VC10.dump_cover(0);</script>
        </binding>            
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation> 
	
	<!-- Turn cover for retract -->
	<animation>
		  <type>rotate</type>
		  <object-name>cover.001</object-name>
		  <property>VC10/fuel/valves/dump-cover[1]</property>
      <factor>90</factor>
		  <axis>
		   <x>0</x>
		   <y>1</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0</x-m>
		  <y-m>0.0</y-m>
		  <z-m>0.028</z-m>
		  </center>  
	</animation>

  <animation>
      <type>translate</type>
      <object-name>cover.001</object-name>
      <property>VC10/fuel/valves/dump-cover[1]</property>
			<interpolation>
				<entry><ind> 0.0</ind><dep> 0.0</dep></entry>
				<entry><ind> 0.8</ind><dep> 0.0</dep></entry>
				<entry><ind> 1.0</ind><dep> 0.008</dep></entry>
			</interpolation>
      <axis>
          <x> 0.0</x>
          <y> 1.0</y>
          <z> 0.0</z>
      </axis>
      <center>
				<x-m>0.0</x-m>
				<y-m>0.0</y-m>
				<z-m>0.023</z-m>
      </center>
  </animation>

	<animation>
		  <type>pick</type>
		  <object-name>cover.001</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>       
		    <binding>
            <command>nasal</command>
            <script>VC10.dump_cover(1);</script>
        </binding>            
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation> 
	
	<!-- Left Centre Tank valve -->
	<animation>
		  <type>rotate</type>
		  <object-name>switch.001</object-name>
		  <property>VC10/fuel/valves/dump-valve[0]</property>
      <factor>-60</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
