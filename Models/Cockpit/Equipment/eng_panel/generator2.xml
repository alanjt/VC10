<PropertyList>
 <path>generator.ac</path>

	<animation>
		<type>material</type>
		<object-name>LampAmber</object-name>
		<object-name>LampAmber.001</object-name>
		<object-name>LampAmber.002</object-name>
		<object-name>sockle.lamp</object-name>
		<object-name>sockle</object-name>
		<object-name>sockle.001</object-name>
		<object-name>sockle.002</object-name>  
		<object-name>switch</object-name>  
		<object-name>switch.001</object-name>  
		<object-name>switch.002</object-name>
		<object-name>cover</object-name>
		<object-name>cover.001</object-name>
		<object-name>bezel</object-name>
		<object-name>freqKnob</object-name>
		<condition>	
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.2</red>
		  <green>0.2</green>
		  <blue>0.2</blue>
		  <factor-prop>systems/electrical/outputs/engineer-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>Needle</object-name>
		<condition>
			<less-than><property>systems/electrical/outputs/cabin-dim</property><value>0.8</value></less-than>
		</condition>
		<emission>
			<red>0.6</red>
			<green>0.4</green>
			<blue>0.3</blue>
			<factor-prop>systems/electrical/outputs/engineer-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>Needle</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<property>systems/electrical/outputs/engineer-dim</property>
		</condition>
		<emission>
		  <red>0.8</red>
		  <green>0.8</green>
		  <blue>0.8</blue>
		  <factor-prop>systems/electrical/outputs/engineer-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>noshadow</type>
		<object-name>LampAmber</object-name>		
		<object-name>LampAmber.001</object-name>
		<object-name>LampAmber.002</object-name>
		<object-name>LampAmberON</object-name>
		<object-name>LampAmberON.001</object-name>
		<object-name>LampAmberON.002</object-name>
	</animation>

	<animation>
		<type>material</type>
		<object-name>LampAmberON</object-name>
		<object-name>LampAmberON.001</object-name>
		<object-name>LampAmberON.002</object-name>
		<emission>
		  <red>   1.0 </red>
		  <green> 0.8 </green>
		  <blue>  0.4 </blue>
		</emission>
	</animation>
	
  <!-- Bus Tie Breaker switch -->
  <animation>
		<type>select</type>
		<object-name>LampAmberON</object-name>
		<condition>			
			<greater-than>
				<property>VC10/electric/ess-bus</property>
				<value>20</value>
			</greater-than>
		  <or>
		  	<less-than>
		    	<property>VC10/electric/ac/BTB2-relay</property>
		    	<value>1</value>
		    </less-than>
		    <property>controls/test/warn-knob</property>
		  </or>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>LampAmber</object-name>
		<condition>
		  <or>
				<less-than>
					<property>VC10/electric/ess-bus</property>
					<value>20</value>
				</less-than>
				<and>
				  <greater-than>
				  	<property>VC10/electric/ac/BTB2-relay</property>
				  	<value>0</value>
				  </greater-than>
				  <not><property>controls/test/warn-knob</property></not>
				</and>
			</or>
		</condition>
	</animation>
	
  <animation>
    <type>rotate</type>
    <object-name>cover</object-name>
    <property>VC10/electric/ac/generator/gen-bus-tie-cover[1]</property>
    <interpolation> 
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-90.0</dep>
            </entry>		
        </interpolation>
    <axis>
     <x> 1</x>
     <y> 0</y>
     <z> 0</z>
    </axis>
    <center>
    <x-m> 0.0</x-m>
    <y-m>-0.0003</y-m>
    <z-m> 0.0153</z-m>
    </center>  
  </animation>

	<animation>
		  <type>pick</type>
		  <object-name>cover</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
		        <property>VC10/electric/ac/generator/gen-bus-tie-cover[1]</property>
		    </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch3</property>
		    </binding>
		  </action>
	</animation>

	<animation>
		  <type>rotate</type>
		  <object-name>switch</object-name>
		  <property>VC10/electric/ac/BTB2-relay</property>
      <factor>-40</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0</x-m>
		  <y-m>0.0</y-m>
		  <z-m>0.0</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>switch</object-name>
		  <object-name>sockle</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>
            	var bt = getprop("VC10/electric/ac/BTB2-relay") or 0;
            	if(bt > 0){
            		setprop("VC10/electric/ac/BTB2-relay", 0);
            	}else{
            		setprop("VC10/electric/ac/BTB2-relay", 2);
            	}
            </script>
        </binding>
        <mod-up>
          <binding>
            <command>nasal</command>
            <script>
            	var bt = getprop("VC10/electric/ac/BTB2-relay") or 0;
            	if(bt == 2){
            		setprop("VC10/electric/ac/BTB2-relay", 1);
            	}else{
            		setprop("VC10/electric/ac/BTB2-relay", 0);
            	}
            </script>
          </binding>
        </mod-up>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
  <!-- Generator (without cover) Breaker switch -->
  <animation>
		<type>select</type>
		<object-name>LampAmberON.001</object-name>
		<condition>		
			<greater-than>
				<property>VC10/electric/ess-bus</property>
				<value>20</value>
			</greater-than>
		  <or>
		  	<less-than>
		    	<property>VC10/electric/ac/generator/GCR2-sw</property>
		    	<value>1</value>
		    </less-than>
		    <property>controls/test/warn-knob</property>
		  </or>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>LampAmber.001</object-name>
		<condition>		  
			<or>
				<less-than>
					<property>VC10/electric/ess-bus</property>
					<value>20</value>
				</less-than>
				<and>
				  <greater-than>
				  	<property>VC10/electric/ac/generator/GCR2-sw</property>
				  	<value>0</value>
				  </greater-than>
				  <not><property>controls/test/warn-knob</property></not>
				</and>
			</or>
		</condition>
	</animation>

	<animation>
		  <type>rotate</type>
		  <object-name>switch.002</object-name>
		  <property>VC10/electric/ac/generator/GCR2-sw</property>
      <factor>-40</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0</x-m>
		  <y-m>-0.0011</y-m>
		  <z-m>-0.06</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>switch.002</object-name>
		  <object-name>sockle.002</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>
            	var gb = getprop("VC10/electric/ac/generator/GCR2-sw") or 0;
            	if(gb > 0){
            		setprop("VC10/electric/ac/generator/GCR2-sw", 0);
            	}else{
            		setprop("VC10/electric/ac/generator/GCR2-sw", 2);
            	}
            </script>
        </binding>
        <mod-up>
          <binding>
            <command>nasal</command>
            <script>
            	var gb = getprop("VC10/electric/ac/generator/GCR2-sw") or 0;
            	if(gb == 2){
            		setprop("VC10/electric/ac/generator/GCR2-sw", 1);
            	}else{
            		setprop("VC10/electric/ac/generator/GCR2-sw", 0);
            	}
            </script>
          </binding>
        </mod-up>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>

  <!-- Generator Controll switch -->
  <animation>
		<type>select</type>
		<object-name>LampAmberON.002</object-name>
		<condition>
			<greater-than>
				<property>VC10/electric/ess-bus</property>
				<value>20</value>
			</greater-than>
		  <or>
		  	<less-than>
		    	<property>VC10/electric/ac/generator/Gen2Control-sw</property>
		    	<value>1</value>
		    </less-than>
		    <property>controls/test/warn-knob</property>
		  </or>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>LampAmber.002</object-name>
		<condition>
			<or>
				<less-than>
					<property>VC10/electric/ess-bus</property>
					<value>20</value>
				</less-than>
				<and>
				  <greater-than>
				  	<property>VC10/electric/ac/generator/Gen2Control-sw</property>
				  	<value>0</value>
				  </greater-than>
				  <not><property>controls/test/warn-knob</property></not>
				</and>
			</or>
		</condition>
	</animation>
	
  <animation>
    <type>rotate</type>
    <object-name>cover.001</object-name>
    <property>VC10/electric/ac/generator/gen-control-cover[1]</property>
    <interpolation> 
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-90.0</dep>
            </entry>		
        </interpolation>
    <axis>
     <x> 1</x>
     <y> 0</y>
     <z> 0</z>
    </axis>
    <center>
    <x-m> 0.0</x-m>
    <y-m>-0.0014</y-m>
    <z-m>-0.086</z-m>
    </center>  
  </animation>

	<animation>
		  <type>pick</type>
		  <object-name>cover.001</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
		        <property>VC10/electric/ac/generator/gen-control-cover[1]</property>
		    </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch3</property>
		    </binding>
		  </action>
	</animation>

	<animation>
		  <type>rotate</type>
		  <object-name>switch.001</object-name>
		  <property>VC10/electric/ac/generator/Gen2Control-sw</property>
      <factor>-40</factor>
		  <axis>
		   <x>1</x>
		   <y>0</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0</x-m>
		  <y-m>-0.0015</y-m>
		  <z-m>-0.1018</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>switch.001</object-name>
		  <object-name>sockle.001</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>
            	var gc = getprop("VC10/electric/ac/generator/Gen2Control-sw") or 0;
            	if(gc > 0){
            		setprop("VC10/electric/ac/generator/Gen2Control-sw", 0);
            	}else{
            		setprop("VC10/electric/ac/generator/Gen2Control-sw", 2);
            		VC10.ac_sync();
            	}
            </script>
        </binding>
        <mod-up>
          <binding>
            <command>nasal</command>
            <script>
            	var gc = getprop("VC10/electric/ac/generator/Gen2Control-sw") or 0;
            	if(gc == 2){
            		setprop("VC10/electric/ac/generator/Gen2Control-sw", 1);
            		VC10.ac_sync();
            	}else{
            		setprop("VC10/electric/ac/generator/Gen2Control-sw", 0);
            	}
            </script>
          </binding>
        </mod-up>
		    <binding>
		      <command>property-toggle</command>
		      <property>sim/sound/switch2</property>
		    </binding>
		  </action>
	</animation>
	
	<!-- generator load in kw/kvars -->
	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
		<property>VC10/electric/ac/generator/Gen2-kw</property>
		<interpolation>
		  <entry><ind> -0.1</ind><dep>  0.0</dep></entry>
		  <entry><ind>  0.0</ind><dep> 15.0</dep></entry>
		  <entry><ind> 50.0</ind><dep>115.0</dep></entry>
		</interpolation>    
		<center>
		  <x-m>-0.0122</x-m>
		  <y-m> 0.0</y-m>
		  <z-m>-0.1667</z-m>
    </center>  
		<axis>
		  <x>0</x>
		  <y>1</y>
		  <z>0</z>
		</axis>
	</animation>

	<!-- the generator frequency knob -->
	<animation>
		  <type>rotate</type>
		  <object-name>freqKnob</object-name>
		  <property>VC10/electric/ac/generator/Gen2-freq</property>
			<interpolation>
				<entry><ind>  380</ind><dep> 0.0</dep></entry>
				<entry><ind>  420</ind><dep> 180</dep></entry>
			</interpolation> 
		  <axis>
		   <x>0</x>
		   <y>1</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>-0.00664</x-m>
		  <y-m> 0.0</y-m>
		  <z-m>-0.209</z-m>
		  </center>  
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>freqKnob</object-name>
		<visible>true</visible>
		  <action>
		    <name>calibrate reset</name>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		      <command>property-assign</command>
		      <property>VC10/electric/ac/generator/Gen2-freq</property>
		      <value>380</value>
		    </binding>          
		    <binding>
          <command>nasal</command>
          <script>VC10.ac_sync();</script>
        </binding>           
		    <binding>
          <command>property-toggle</command>
          <property>sim/sound/switch3</property>
        </binding>
		  </action>      
		  <action>
		    <name>calibrate fine dec</name>
		    <button>3</button>
		    <repeatable>true</repeatable>
		    <binding>
		      <command>property-adjust</command>
		      <property>VC10/electric/ac/generator/Gen2-freq</property>
		      <step>0.5</step>
		      <min>380</min>
		      <max>420</max>
		      <wrap>false</wrap>
		    </binding>          
		    <binding>
          <command>nasal</command>
          <script>VC10.ac_sync();</script>
        </binding>            
		    <binding>
          <command>property-toggle</command>
          <property>sim/sound/switch3</property>
        </binding>
		  </action>
		  <action>
		    <name>calibrate fine inc</name>
		    <button>4</button>
		    <repeatable>true</repeatable>
		    <binding>
		      <command>property-adjust</command>
		      <property>VC10/electric/ac/generator/Gen2-freq</property>
		      <step>-0.5</step>
		      <min>380</min>
		      <max>420</max>
		      <wrap>false</wrap>
		    </binding>          
		    <binding>
          <command>nasal</command>
          <script>VC10.ac_sync();</script>
        </binding>            
		    <binding>
          <command>property-toggle</command>
          <property>sim/sound/switch3</property>
        </binding>
		  </action>
	</animation>
	
</PropertyList>
