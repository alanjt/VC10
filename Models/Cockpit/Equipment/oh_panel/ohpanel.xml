<PropertyList>
 	<path>ohpanel.ac</path>
 	
	<animation>
		<type>material</type>
		<object-name>ohpanel</object-name>
		<condition>	
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.3</red>
		  <green>0.3</green>
		  <blue>0.3</blue>
		  <factor-prop>systems/electrical/outputs/overhead-dim</factor-prop>
		</emission>
	</animation>
	
	<model>
		<name>transponder</name>
		<path>Aircraft/VC10/Models/Cockpit/Equipment/oh_panel/transponder.xml</path>
		<offsets>
		  <x-m> 0.0</x-m>
		  <y-m> 0.0</y-m>
		  <z-m> 0.0</z-m>
		</offsets>
	</model>

	<model>
		<name>emer_lights</name>
		<path>Aircraft/VC10/Models/Cockpit/Equipment/oh_panel/emerYawAndMore.xml</path>
		<offsets>
		  <x-m> 0.0</x-m>
		  <y-m> 0.0</y-m>
		  <z-m> 0.0</z-m>
		</offsets>
	</model>
	
	<model>
		<name>window_heat</name>
		<path>Aircraft/VC10/Models/Cockpit/Equipment/oh_panel/windowHeat.xml</path>
