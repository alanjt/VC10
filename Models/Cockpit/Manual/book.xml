	<!-- page number -->
	<model>
	  <name>pageNumber</name>
		<path>Aircraft/VC10/Models/Cockpit/Manual/layout.xml</path>
		<offsets>
			<x-m> 0.14</x-m>
			<y-m>-0.10</y-m>
			<z-m> 0.0131</z-m>
		</offsets>
		<overlay>
			<params>
				<property type="string">VC10/manual/page</property>
				<character-size type="double">0.007</character-size>
				<color>
					<red   type="double">0.0</red>
					<green type="double">0.0</green>
					<blue  type="double">0.0</blue>
				</color>
				<alignment>center-center</alignment>
			</params>
		</overlay>
	</model>

	<animation>
		<type>select</type>
		<object-name>pageNumber</object-name>
		<condition>	
			<greater-than>
				<property>VC10/manual/page</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	
