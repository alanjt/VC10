<PropertyList>
 	<path>book.ac</path>
 	
	<animation>
		<type>material</type>
		<object-name>back</object-name>
		<object-name>board</object-name>
		<object-name>bracket</object-name>
		<object-name>pages</object-name>
		<object-name>pages.001</object-name>
		<object-name>saddle</object-name>
		<object-name>flagGB</object-name>
		<object-name>flagGER</object-name>
		<object-name>flagES</object-name>
		<object-name>flagCZ</object-name>
		<object-name>flagPOR</object-name>
		<object-name>flagITA</object-name>
		<condition>	
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.3</red>
		  <green>0.3</green>
		  <blue>0.3</blue>
		  <factor-prop>systems/electrical/outputs/engineer-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>pages.001</object-name>
		<condition>	
			<and>	
				<greater-than>
					<property>VC10/manual/page</property>
					<value>0</value>
				</greater-than>
				<greater-than>
		  		<property>VC10/manual/board</property>
					<value>0.94</value>
				</greater-than>
			</and>
		</condition>
	</animation>
	
	<!-- Open cover -->
	<animation>
		  <type>rotate</type>
		  <object-name>board</object-name>
		  <object-name>saddle</object-name>
		  <property>VC10/manual/board</property>
			<interpolation>
				<entry><ind>0.0</ind><dep>  0.0</dep></entry>
				<entry><ind>0.6</ind><dep>-100.0</dep></entry>
			</interpolation>
		  <axis>
		   <x>0</x>
		   <y>1</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0004</x-m>
		  <y-m>0.0</y-m>
		  <z-m>0.0036</z-m>
		  </center>  
	</animation>
	
	<animation>
		  <type>rotate</type>
		  <object-name>board</object-name>
		  <property>VC10/manual/board</property>
			<interpolation>
				<entry><ind>0.6</ind><dep>   0.0</dep></entry>
				<entry><ind>1.0</ind><dep> -85.0</dep></entry>
			</interpolation>
		  <axis>
		   <x>0</x>
		   <y>1</y>
		   <z>0</z>
		  </axis>
		  <center>
		  <x-m>0.0068</x-m>
		  <y-m>0.0</y-m>
		  <z-m>0.0445</z-m>
		  </center>  
	</animation>

	<animation>
		  <type>pick</type>
		  <object-name>board</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
        <binding>
            <command>nasal</command>
            <script>
            	var b = getprop("VC10/manual/board") or 0;
            	if(b){
            		interpolate("VC10/manual/board", 0, 0.4);
            	}else{
            		interpolate("VC10/manual/board", 1, 0.4);
            	}
            </script>
        </binding>          
		  </action>
	</animation> 

	<animation>
		  <type>pick</type>
		  <object-name>flagGB</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
        <binding>
				  <command>property-assign</command>
				  <property>VC10/manual/page</property>
				  <value>2</value>
        </binding> 
				<binding>
				  <command>nasal</command>
				  <script>VC10.manual();</script>
				</binding>	         
		  </action>
	</animation> 	

	<animation>
		  <type>pick</type>
		  <object-name>flagES</object-name>
