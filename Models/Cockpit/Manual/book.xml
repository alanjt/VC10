	<animation>
		<type>pick</type>
		<object-name>pages</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
		    <property>VC10/manual/page</property>
		    <step>1</step>
		    <min>0</min>
		    <max>100</max>
		    <wrap>false</wrap>
		  </binding>
		  <binding>
		    <command>nasal</command>
		    <script>VC10.manual();</script>
		  </binding>					
		</action>
		<action>
		  <button>3</button>
