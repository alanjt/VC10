<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>

	<model>
		<name>cockpitfloor</name>
		<path>CockpitFloor.ac</path>
		<offsets>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</offsets>
	</model>
<!-- Hide cockpit floor plan which is only used to position cockpit items -->	
	<animation>
		<type>select</type>
		<object-name>Cockpit_FloorPlan</object-name>
		<condition>
		<equals>
			<value>1</value><value>0</value>
		</equals>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Cockpit_Floor</object-name>
		<condition>
		<equals>
			<value>1</value><value>1</value>
		</equals>
		</condition>
	</animation>
	
</PropertyList>