    <!-- The parking brake -->
		<animation>
			<type>translate</type>
			<object-name>PBrakeLever</object-name>
      <property>controls/gear/brake-parking</property>
      <factor>0.02</factor>
			<axis>
				<x>0</x>
				<y>0</y>
				<z>1</z>
			</axis>
		</animation>

    <animation>
      <type>pick</type>
      <object-name>PBrakeLever</object-name>
      <visible>true</visible>
      <action>
        <button>0</button>
        <repeatable>false</repeatable>
        <binding>
          <command>property-toggle</command>
          <property>controls/gear/brake-parking</property>  
        </binding>
        <binding>
          <command>nasal</command>
          <script>
          	var brakeR = getprop("/controls/gear/brake-right") or 0;
          	var brakeL = getprop("/controls/gear/brake-left") or 0;
          	var stop = getprop("/controls/gear/barke-parking") or 0;
          	if (!stop and (brakeR or brakeL)){
          		setprop("/controls/gear/brake-right", 0);
          		setprop("/controls/gear/brake-left", 0);
            }
          </script>  
        </binding>
        <binding>
          <command>property-toggle</command>
          <property>sim/sound/switch1</property>
        </binding>
      </action>
    </animation>
    
  <!-- speedbrake lever -->
  <animation>
		<type>rotate</type>
		<object-name>BrkLever</object-name>
		<property>controls/flight/spoilers</property>
		<factor> 60</factor>
		<center>
		  <x-m>-18.6285</x-m>
		  <y-m>0.0</y-m>
		  <z-m>0.6411</z-m>
		</center>
		<axis>
		  <x>0.0</x>
		  <y>1.0</y>
		  <z>0.0</z>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>BrkLever</object-name>
		<action>
		  <button>0</button>
		  <repeatable>false</repeatable>
		  <binding>
		    <command>nasal</command>
		    <script>VC10.stepSpeedbrakes(1)</script>
		  </binding>
		  <mod-up>
		  <binding>
		    <command>nasal</command>
		    <script>VC10.stepSpeedbrakes(0)</script>
		  </binding>
		  </mod-up>
		</action>
		<action>
		  <button>1</button>
		  <repeatable>false</repeatable>
		  <binding>
		    <command>nasal</command>
		    <script>VC10.stepSpeedbrakes(-1)</script>
		  </binding>
		  <mod-up>
		  <binding>
		    <command>nasal</command>
		    <script>VC10.stepSpeedbrakes(0)</script>
		  </binding>
		  </mod-up>
		</action>
	</animation>
	
