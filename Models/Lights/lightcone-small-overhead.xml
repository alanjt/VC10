<PropertyList>
 <path>lightcone-small.ac</path>
	<animation>
		 <type>light</type>
		 <light-type>spot</light-type>
		 <object-name>lightcone-small</object-name>
		 <nopreview/>
		 <position>
		   <x>0.0</x>
		   <y>0.0</y>
		   <z>0.0</z>
		 </position>
		 <direction>
		   <x>0.0</x>
		   <y>0.0</y>
		   <z>-1.0</z>
		 </direction>
		 <ambient>
		   <r>0.1</r>
		   <g>0.1</g>
		   <b>0.1</b>
		   <a>1.0</a>
		 </ambient>
		 <diffuse>
		   <r>0.9</r>
		   <g>0.9</g>
		   <b>0.8</b>
		   <a>1.0</a>
		 </diffuse>
		 <specular>
		   <r>0.9</r>
		   <g>0.9</g>
		   <b>0.9</b>
		   <a>1.0</a>
		 </specular>
		 <dim-factor>
			 <property>systems/electrical/outputs/overhead-dim</property>
			 <factor>1</factor>
			 <offset>0</offset>
			 <min>0</min>
			 <max>1</max>
		 </dim-factor>
		 <attenuation>
		   <c>1.0</c>
		   <l>0.002</l>
		   <q>0.00005</q>
		 </attenuation>
		 <exponent>30.0</exponent>
		 <cutoff>60.0</cutoff>
		 <near-m>0.1</near-m>
		 <far-m>2.0</far-m>
	 </animation>
</PropertyList> 
